	file = {:Users/fred/Library/Mobile Documents/com\~{}apple\~{}CloudDocs/Mendeley Desktop/2006/Letzkus et al. - 2006 - Lateralization of olfaction in the honeybee Apis mellifera.pdf:pdf},
	issn = {0960-9822},
	journal = {Current biology : CB},
	keywords = {Animals,Bees,Bees: anatomy \& histology,Bees: cytology,Bees: physiology,Behavior, Animal,Flagella,Flagella: ultrastructure,Functional Laterality,Learning,Reward,Smell,Smell: physiology,Stimulation, Chemical},
	month = jul,
	number = {14},
	pages = {1471--6},
	pmid = {16860748},
	title = {{Lateralization of olfaction in the honeybee Apis mellifera.}},
