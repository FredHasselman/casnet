	keywords = {1996,1999,2010 for a review,are often interpreted as,bak,car,card sort,evidence,executive control,fractally scaled,intermittent fluctuations in biological,multifractal,multiplicative,of self-organized criticality,see yates,self-organized criticality,soc,sol\'{e} et al,systems},
	month = jan,
