	keywords = {Chemistry, Physical,Chemistry, Physical: methods,Computer Simulation,Dimyristoylphosphatidylcholine,Dimyristoylphosphatidylcholine: chemistry,Lipid Bilayers,Lipid Bilayers: chemistry,Models, Chemical,Models, Molecular,Models, Statistical,Molecular Conformation,Phospholipids,Phospholipids: chemistry},
