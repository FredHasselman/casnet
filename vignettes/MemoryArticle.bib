	issn = {1092-4388},
	journal = {Journal of speech, language, and hearing research : JSLHR},
	keywords = {auditory,saturation effect,sibilant perception,sibilant production},
	pages = {1259--1269},
	pmid = {15842009},
	title = {{The distinctness of speakers' /s/-/S/ contrast is related to their auditory discrimination and use of an articulatory saturation effect.}},
	volume = {47},
