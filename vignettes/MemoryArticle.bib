	keywords = {Animals,Brain,Brain: physiology,DNA,DNA: genetics,DNA: physiology,Genetics,Genetics: trends,History, 20th Century,Humans,Information Dissemination,Physiology,Physiology: history,Physiology: trends,Systems Biology,Systems Biology: history},
	month = jan,
	number = {1},
	pages = {16--26},
	pmid = {17951329},
	title = {{Claude Bernard, the first systems biologist, and the future of physiology.}},
