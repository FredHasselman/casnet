	keywords = {a prominent,a-p,abbreviations,anteroposterior,c,caenorhabditis elegans,elegans,embryonic,embryonic development,l-r,left-right,left-right axis,of the primary,par,partitioning of cytoplasm,pcp,planar cell polarity,structures and forms independently,symmetry breaking,wnt signaling},
	month = jan,
	number = {1},
	pages = {34--40},
	pmid = {21509174},
