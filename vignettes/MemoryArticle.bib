	doi = {10.1121/1.1787524},
	file = {:Users/fredhasselman/Library/Mobile Documents/com\~{}apple\~{}CloudDocs/Mendeley Desktop/2004/Perkell et al. - 2004 - The distinctness of speakers' productions of vowel contrasts is related to their discrimination of the contrasts.pdf:pdf},
	issn = {00014966},
	journal = {The Journal of the Acoustical Society of America},
	number = {4},
	pages = {2338--2344},
	pmid = {15532664},
	title = {{The distinctness of speakers' productions of vowel contrasts is related to their discrimination of the contrasts.}},
	volume = {116},
