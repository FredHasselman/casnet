	keywords = {Adolescent,Adult,Eye,Female,Hand,Hand: physiology,Humans,Male,Memory,Memory: physiology,Movement,Movement: physiology,Ocular Physiological Phenomena,Psychomotor Performance,Psychomotor Performance: physiology},
