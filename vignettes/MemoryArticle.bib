	keywords = {Action Potentials,Action Potentials: physiology,Brain,Brain: physiology,Electroencephalography,Humans,Models, Neurological,Neurons,Neurons: physiology,Space Perception,Space Perception: physiology,Synapses,Synapses: physiology,Time Perception,Time Perception: physiology},
	month = jan,
	number = {3},
	pages = {1282--7},
	pmid = {11158631},
	title = {{What is a moment? Transient synchrony as a collective mechanism for spatiotemporal integration.}},
