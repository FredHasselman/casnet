	journal = {PLoS computational biology},
	keywords = {Algorithms,Animals,Brain,Brain: anatomy \& histology,Brain: physiology,Humans,Linear Models,Mental Processes,Mental Processes: physiology,Models, Neurological,Nerve Net,Nerve Net: anatomy \& histology,Nerve Net: physiology,Neural Networks (Computer),Nonlinear Dynamics,Probability},
	month = nov,
