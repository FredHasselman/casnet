	issn = {1664-042X},
	journal = {Frontiers in physiology},
	keywords = {1,1/f noise,accuracy trade-off,f noise,kinematics,m,motor control,self-organization,speed,speed--accuracy trade-off},
	mendeley-groups = {WP-Publications,2014-PeerJ},
