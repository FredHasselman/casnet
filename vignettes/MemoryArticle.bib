@article{Freeman2008a,
	abstract = {Computational models of brain dynamics fall short of performance in speed and robustness of pattern recognition in detecting minute but highly significant pattern fragments. A novel model employs the properties of thermodynamic systems operating far from equilibrium, which is analyzed by linearization near adaptive operating points using root locus techniques. Such systems construct order by dissipating energy. Reinforcement learning of conditioned stimuli creates a landscape of attractors and their basins in each sensory cortex by forming nerve cell assemblies in cortical connectivity. Retrieval of a selected category of stored knowledge is by a phase transition that is induced by a conditioned stimulus, and that leads to pattern self-organization. Near self-regulated criticality the cortical background activity displays aperiodic null spikes at which analytic amplitude nears zero, and which constitute a form of Rayleigh noise. Phase transitions in recognition and recall are initiated at null spikes in the presence of an input signal, owing to the high signal-to-noise ratio that facilitates capture of cortex by an attractor, even by very weak activity that is typically evoked by a conditioned stimulus.},
	author = {Freeman, Walter J},
	date-modified = {2014-11-17 12:18:57 +0000},
	file = {:Users/fred/Library/Mobile Documents/com\~{}apple\~{}CloudDocs/Mendeley Desktop/2008/Freeman - 2008 - A pseudo-equilibrium thermodynamic model of information processing in nonlinear brain dynamics.pdf:pdf},
	issn = {0893-6080},
	journal = {Neural networks : the official journal of the International Neural Network Society},
	keywords = {Animals,Automatic Data Processing,Automatic Data Processing: methods,Biological,Brain,Brain Mapping,Brain: cytology,Brain: physiology,Cats,Dendrites,Dendrites: physiology,Electroencephalography,Humans,Models,Neurons,Neurons: cytology,Nonlinear Dynamics,Spectrum Analysis,Thermodynamics,brain waves,electroencephalogram,nonequilibrium thermodynamics,null spike,phase transition,rayleigh noise,root locus},
	number = {2-3},
	pages = {257--65},
	pmid = {18249088},
	title = {{A pseudo-equilibrium thermodynamic model of information processing in nonlinear brain dynamics.}},
	volume = {21},
	year = {2008},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/18249088},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.neunet.2007.12.011}}

@article{Freeman1997a,
	abstract = {Explores history of the machine metaphor for mind/brain function, beginning with R. Descartes' proposal of the brain as a pump for fluids termed animal spirits, controlled by the soul in humans, but not in animals who were seen as soulless machines. In the late 18th century, a theory of reflex action based on Newtonian optics viewed the brain as a complex but passive mirror. After demonstration of the conservation of energy, a new doctrine of nerve forces and energies replaced that of spirits and nerves carrying spiritual forces. (The conservation of momentum principle was used by Freud as a foundation for his scientific psychology.) Failure of the energy metaphor led to the concept of nerve cells operating as binary switches in neural networks, with the term information used to describe neural activity. Inadequacies of the input-output approaches to brain dynamics are outlined, focusing on recognition of the uniqueness of brain function, followed by description of nonlinear brain dynamics and neuroexistentialism, and the neurobiology theory of self-organization of mind and brain. Problems that remain in the biology of intentionality are reviewed. (PsycINFO Database Record (c) 2010 APA, all rights reserved)},
