	Issn = {1432-1106},
	Journal = {Experimental brain research},
	Keywords = {Adult,Analysis of Variance,Cooperative Behavior,Female,Hand,Hand: physiology,Humans,Interpersonal Relations,Male,Motivation,Motivation: physiology,Movement,Movement: physiology,Periodicity,Psychomotor Performance,Psychomotor Performance: physiology,Statistics as Topic,Time Factors,Young Adult},
	Month = oct,
	Number = {1-2},
	Pages = {137--48},
	Pmid = {22865163},
	Title = {{Strong anticipation: complexity matching in interpersonal coordination.}},
