	Issn = {00954470},
	Journal = {Journal of Phonetics},
	Keywords = {Adaptive resonance,Auditory scene analysis,Categorization,Consciousness,Speech perception,Word recognition},
	Month = oct,
	Number = {3-4},
	Pages = {423--445},
	Title = {{Resonant neural dynamics of speech perception}},
