	Isbn = {1099-0909 (Electronic)$\backslash$r1076-9242 (Linking)},
	Issn = {1099-0909},
	Journal = {Dyslexia (Chichester, England)},
	Keywords = {dyslexia,naming,orthography,phonological awareness,reading fluency},
	Number = {3},
	Pages = {258--282},
	Pmid = {20680995},
	Title = {{Interaction of phonological awareness and 'magnocellular' processing during normal and dyslexic reading: behavioural and fMRI investigations.}},
	Volume = {16},
	Year = {2010},
	Bdsk-Url-1 = {https://dx.doi.org/10.1002/dys}}

