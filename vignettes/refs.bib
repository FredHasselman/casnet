	Issn = {00100277},
	Journal = {Cognition},
	Keywords = {Categorical perception,Infancy,Speech perception},
	Month = mar,
	Number = {2},
	Pages = {B15--26},
	Pmid = {15694642},
	Title = {{Infants are sensitive to within-category variation in speech perception}},
