	Issn = {0964-6906},
	Journal = {Human molecular genetics},
	Keywords = {Animals,Diseases in Twins,Environment,Epigenesis, Genetic,Genetic Variation,Humans,Models, Genetic,Phenotype,Sequence Analysis, DNA,Twins, Dizygotic,Twins, Monozygotic},
