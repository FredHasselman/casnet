	Journal = {Trends in Neurosciences},
	Month = may,
	Number = {4},
	Pages = {147--152},
	Pmid = {9106353},
	Publisher = {Elsevier},
	Title = {{To see but not to read; the magnocellular theory of dyslexia}},
