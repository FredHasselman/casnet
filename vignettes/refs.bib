	Isbn = {0027-8424},
	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {Adult,Autopsy,Dyslexia,Dyslexia: pathology,Dyslexia: physiopathology,Evoked Potentials,Female,Geniculate Bodies,Geniculate Bodies: pathology,Geniculate Bodies: physiology,Humans,Male,Visual Pathways,Visual Pathways: physiology,Visual Perception,Visual Perception: physiology},
