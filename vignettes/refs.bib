	File = {:Users/fred/Library/Mobile Documents/com\~{}apple\~{}CloudDocs/Mendeley Desktop/2010/Ihlen, Vereijken - 2010 - Interaction-dominant dynamics in human cognition beyond 1f(alpha) fluctuation.pdf:pdf},
	Issn = {1939-2222},
	Journal = {Journal of experimental psychology. General},
	Keywords = {1,ability to adapt,cognitive performance,common feature of complex,dynamics,f,fluctuation,interaction-dominant,is a,multiplicative interactions,response time,structures and temporal functions,systems and reflects their,the emergence of spatial},
	Month = aug,
	Number = {3},
	Pages = {436--63},
	Pmid = {20677894},
	Title = {{Interaction-dominant dynamics in human cognition: beyond 1/f(alpha) fluctuation.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/20677894},
	Volume = {139},
