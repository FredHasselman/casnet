	File = {:Users/fred/Library/Mobile Documents/com\~{}apple\~{}CloudDocs/Mendeley Desktop/2001/Freeman, Kozma, Werbos - 2001 - Biocomplexity adaptive behavior in complex stochastic dynamical systems.pdf:pdf},
	Issn = {0303-2647},
