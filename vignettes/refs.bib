	Isbn = {1088-8438},
	Issn = {1088-8438},
	Journal = {Scientific Studies of Reading},
	Number = {1},
	Pages = {1--25},
	Title = {{RAN Is Not a Measure of Orthographic Processing. Evidence From the Asymmetric German Orthography}},
