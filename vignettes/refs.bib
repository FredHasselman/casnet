@article{port2010a,
	Abstract = {It is proposed that a language, in a rich, high-dimensional form,$\backslash$nis part of the cultural environment of the child learner. A language$\backslash$nis the product of a community of speakers who develop its phonological,$\backslash$nlexical, and phrasal patterns over many generations. The language$\backslash$nemerges from the joint behavior of many agents in the community acting$\backslash$nas a complex adaptive system. Its form only roughly approximates$\backslash$nthe low-dimensional structures that our traditional phonology highlights.$\backslash$nThose who study spoken language have attempted to approach it as$\backslash$nan internal knowledge structure rather than as a communal institution$\backslash$nor set of conventions for coordination of activity. We also find$\backslash$nit very difficult to avoid being deceived into seeing language in$\backslash$nthe form employed by our writing system as letters, words, and sentences.$\backslash$nBut our writing system is a further set of conventions that approximate$\backslash$nthe high-dimensional spoken language in a consistent and regularized$\backslash$ngraphical form. {\copyright} Taylor \& Francis Group, LLC.},
	Author = {Port, Robert F.},
	Date-Added = {2018-01-30 12:18:12 +0000},
