	Issn = {0022-0663},
	Journal = {Journal of Educational Psychology},
	Number = {3},
	Pages = {516--531},
	Title = {{Precursors of reading skill from infancy to first grade in Finnish: Continuity and change in a highly inflected language.}},
	Volume = {99},
	Year = {2007},
	Bdsk-Url-1 = {https://dx.doi.org/10.1037/0022-0663.99.3.516}}

