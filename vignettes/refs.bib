	Journal = {Developmental biology},
	Keywords = {Animals,Body Patterning,Cell Movement,Cell Polarity,Chromatids,Chromatids: genetics,Chromatids: metabolism,Cilia,Cilia: genetics,Cilia: metabolism,Cytoplasm,Cytoplasm: genetics,Cytoplasm: metabolism,Cytoskeleton,Cytoskeleton: genetics,Cytoskeleton: metabolism,Embryonic Development,Gene Expression Regulation, Developmental,Mice,Models, Animal,Models, Biological,Penetrance},
