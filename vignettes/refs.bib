	Issn = {00220965},
	Journal = {Journal of Experimental Child Psychology},
	Keywords = {Double deficit hypothesis,Dyslexia,Naming speed deficit,Phonological awareness deficit,Phonological speed,Reading speed},
	Number = {2},
