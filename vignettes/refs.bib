	Keywords = {Animals,Cell Differentiation,Cloning, Organism,Diploidy,Embryo, Mammalian,Embryo, Mammalian: cytology,Embryonic Stem Cells,Embryonic Stem Cells: cytology,Hematologic Neoplasms,Hematologic Neoplasms: therapy,Humans,Models, Biological,Nuclear Reprogramming,Nuclear Transfer Techniques,Phenotype,Stem Cells,Stem Cells: cytology},
