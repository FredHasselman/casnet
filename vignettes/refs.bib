	Journal = {Neural computation},
	Keywords = {Algorithms,Computer Simulation,Humans,Models, Neurological,Nerve Net,Nerve Net: physiology,Neural Networks (Computer),Nonlinear Dynamics,Problem Solving,Problem Solving: physiology,Time Factors},
	Month = nov,
