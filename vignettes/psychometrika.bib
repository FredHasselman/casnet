	Issn = {1749-8104},
	Journal = {Neural development},
	Keywords = {Animals,Axons,Axons: physiology,Functional Laterality,Functional Laterality: physiology,Green Fluorescent Proteins,Green Fluorescent Proteins: genetics,Habenula,Habenula: cytology,Habenula: embryology,Neural Pathways,Neural Pathways: cytology,Neural Pathways: embryology,Presynaptic Terminals,Presynaptic Terminals: physiology,Zebrafish,Zebrafish: embryology},
