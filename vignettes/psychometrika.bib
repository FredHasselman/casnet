@article{chemero2007a,
	Abstract = {The ecological approach to perception-action is unlike the standard approach in several respects. It takes the animal-in--its--environment as the proper scale for the theory and analysis of perception-action, it eschews symbol based accounts of perception-action, it promotes self-organization as the theory-constitutive metaphor for perception-action, and it employs self-referring, non-predicative definitions in explaining perception-action. The present article details the complexity issues confronted by the ecological approach in terms suggested by Rosen and introduces non-well founded set theory (hyperset theory) as a potentially useful tool for expressing them. The issues and the tool are brought to focus in the concept of affordance that is the basis for explanation of prospective control of action in the ecological approach.},
	Author = {Chemero, Anthony and Turvey, M. T.},
	Date-Added = {2020-03-18 20:14:03 +0100},
