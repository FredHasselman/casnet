@article{freeman1999a,
	Abstract = {According to behavioural theories deriving from pragmatism, gestalt psychology, existentialism, and ecopsychology, knowledge about the world is gained by intentional action followed by learning. In terms of the neurodynamics described here, if the intending of an act comes to awareness through reafference, it is perceived as a cause. If the consequences of an act come to awareness through proprioception and exteroception, they are perceived as an effect. A sequence of such states of awareness comprises consciousness. Awareness of the action-perception cycle provides the cognitive metaphor of linear causality as an agency. Humans apply this metaphor to objects and events in the world to predict and control them, and to assign social responsibility. The nature of self-control is described by breaking the circle into a forward limb, the intentional self and a feedback limb, awareness of the self and its actions. Intentional acts are produced by the self-organized microscopic neural activity of cortical and subcortical components in the brain. Awareness supervenes as a macroscopic ordering state, that defers action until the self-organizing microscopic process has reached closure in reflective prediction. (PsycINFO Database Record (c) 2010 APA, all rights reserved)},
	Author = {Freeman, Walter J},
	Date-Added = {2020-03-18 20:14:03 +0100},
	Date-Modified = {2020-03-18 20:14:03 +0100},
	Isbn = {Print 1355-8250 Imprint Academic; US Electronic},
	Journal = {Journal of Consciousness Studies},
	Keywords = {Cognitive Processes [2340],awareness \& intention \& linear vs circular theorie,of behavior \& neurophysiology in cognition \& perce},
	Number = {11-12},
	Pages = {143--172},
	Title = {{Consciousness, intentionality and causality.}},
	Volume = {6},
	Year = {1999}}

