	File = {:Users/fredhasselman/Library/Mobile Documents/com\~{}apple\~{}CloudDocs/Mendeley Desktop/2004/Perkell et al. - 2004 - The Distinctness of Speakers' s-- S Contrast Is Related to Their Auditory Discrimination and Use of an Articu.pdf:pdf},
	Issn = {1092-4388},
	Journal = {Journal of speech, language, and hearing research : JSLHR},
	Keywords = {auditory,saturation effect,sibilant perception,sibilant production},
