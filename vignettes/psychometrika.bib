	Doi = {10.1038/nature.2013.14302},
	Issn = {1476-4687},
	Pages = {1--2},
	Title = {{Peer reviewers urged to speak their minds}},
	Url = {http://www.nature.com/doifinder/10.1038/nature.2013.14302$\backslash$nhttp://www.nature.com/news/peer-reviewers-urged-to-speak-their-minds-1.14302},
	Year = {2013},
