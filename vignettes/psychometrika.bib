	File = {:Users/fredhasselman/Library/Mobile Documents/com\~{}apple\~{}CloudDocs/Mendeley Desktop/2007/Chemero, Turvey - 2007 - Complexity, Hypersets, and the Ecological Perspective on Perception-Action.pdf:pdf},
	Isbn = {1555-5542},
