	Journal = {Developmental Psychobiology},
	Keywords = {brain,crossfostering,early experience,intergenerational transmission,macaca mulatta,maternal behavior,mother infant relationship,primates,serotonin},
	Number = {2},
	Pages = {165--171},
	Pmid = {17299788},
	Title = {{Intergenerational transmission of maternal behavior in rhesus macaques and its underlying mechanisms}},
