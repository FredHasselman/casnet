	Issn = {10538119},
	Keywords = {Audiovisual integration,Dyslexia,Grapheme-phoneme correspondences,Neural reading network,Reading development},
	Number = {3},
	Pages = {695--703},
	Pmid = {21056673},
	Publisher = {NeuroImage},
	Title = {{The neural signature of orthographic-phonological binding in successful and failing reading development}},
