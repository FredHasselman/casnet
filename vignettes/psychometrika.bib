	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Number = {8},
	Pages = {2554--2558},
	Pmid = {6953413},
	Title = {{Neural networks and physical systems with emergent collective computational abilities.}},
	Volume = {79},
