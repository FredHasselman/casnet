	Isbn = {1475-925X},
	Issn = {1475925X},
	Journal = {Biomedical engineering online},
	Keywords = {Algorithms,Artificial Intelligence,Automated,Automated: methods,Computer-Assisted,Computer-Assisted: methods,Diagnosis,Discriminant Analysis,Fractals,Humans,Nonlinear Dynamics,Pattern Recognition,Reproducibility of Results,Sensitivity and Specificity,Speech Disorders,Speech Disorders: diagnosis,Speech Production Measurement,Speech Production Measurement: methods},
	Month = jan,
