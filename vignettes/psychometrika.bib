	Isbn = {1096-0457 (Electronic)$\backslash$r0022-0965 (Linking)},
	Issn = {00220965},
	Journal = {Journal of Experimental Child Psychology},
	Keywords = {Double deficit hypothesis,Dyslexia,Naming speed deficit,Phonological awareness deficit,Phonological speed,Reading speed},
	Number = {2},
	Pages = {202--221},
	Pmid = {19278686},
	Title = {{Naming problems do not reflect a second independent core deficit in dyslexia: Double deficits explored}},
	Volume = {103},
