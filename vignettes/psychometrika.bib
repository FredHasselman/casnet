	Journal = {Experimental Brain Research},
	Keywords = {Coordination dynamics,Human,Multisensory integration,Rhythmic movement},
	Number = {4},
	Pages = {673--688},
	Pmid = {16528497},
	Title = {{Binding of movement, sound and touch: Multimodal coordination dynamics}},
	Volume = {173},
	Year = {2006},
